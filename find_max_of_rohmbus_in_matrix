#include <stdio.h>

int main()
{
    int mat[6][6]={1,2,3,4,5,6,1,2,3,4,5,6,1,2,3,4,5,6,1,2,3,4,5,6,1,2,3,4,5,6,1,2,3,4,5,6};
    int i,j,k;
    int sum=0;
    int max=0;
    
    for(i=0;i<(6-2);i++)
    {
        for(j=0;j<(6-2);j++)
        {
            printf("loop started\n");
            k=j;
            sum=0;
            sum=sum+mat[i+1][k+1];
            printf("mat[%d][%d] ",i+1,k+1);
            for(k=j;k<3+j;k++)
            {
                printf("mat[%d][%d] ",i,k);
                sum=sum+mat[i][k];
                printf("mat[%d][%d] ",i+2,k);
                sum=sum+mat[i+2][k];
            }
            printf("sum is %d\n",sum);
            printf("loop ended\n");
            if(sum > max)
                max=sum;
        }
        
    }
    printf("max val is %d\n",max);
    return 0;
}
